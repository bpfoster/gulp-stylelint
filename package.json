{
  "name": "@ronilaukkarinen/gulp-stylelint",
  "version": "14.0.8",
  "description": "Gulp plugin for running Stylelint results through various reporters.",
  "main": "src/index.js",
  "files": [
    "/src/*.js"
  ],
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.js\"",
    "tape": "tape \"test/*.spec.js\"",
    "test": "npm run lint && npm run tape",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ronilaukkarinen/gulp-stylelint.git"
  },
  "keywords": [
    "gulpplugin",
    "stylelint",
    "postcss",
    "css"
  ],
  "author": "Oleg Sklyanchuk <sklyanchuk@gmail.com> (http://olegskl.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ronilaukkarinen/gulp-stylelint/issues"
  },
  "homepage": "https://github.com/ronilaukkarinen/gulp-stylelint",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=15.14.0 || >=16.0.0"
  },
  "peerDependencies": {
    "stylelint": "^14.15.0"
  },
  "dependencies": {
    "@jridgewell/trace-mapping": "^0.3.13",
    "ansi-colors": "^4.1.3",
    "fancy-log": "^2.0.0",
    "plugin-error": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-stylelint": "^11.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-regexp": "^1.10.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "jest": "^26.0.1",
    "sinon": "^11.1.2",
    "stylelint": "^14.15.0",
    "stylelint-config-standard": "^29.0.0",
    "tape": "^4.13.0"
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    }
  }
}
